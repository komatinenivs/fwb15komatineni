extends layout

block content
  h1= title
  - var image_names= ["icecream1.jpg","icecream2.jpg","icecream3.jpg","icecream4.jpg","icecream5.jpg"]
  - for(let i=0;i<3;i++){
     - let randValue= Math.floor(Math.random()*5)
     img(src=`/images/${image_names[randValue]}` alt=`Image No ${randValue}`)

  - }